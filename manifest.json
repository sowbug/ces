{
  "manifest_version": 2,

  "name": "Coinbase Enhancement Suite",
  "description": "Adds missing features to Coinbase website.",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["https://coinbase.com/transactions/*"],
      "js": ["fix_transactions.js"],
      "run_at": "document_end"
    }
  ]
}